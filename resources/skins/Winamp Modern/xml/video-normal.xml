    downImage="player.button.video.reattach.pressed"
    hoverImage="player.button.video.reattach.hover"
    tooltip="Reattach Video"
    rectrgn="1"
  />

  <layer
    x="-24" y="4"
    relatx="1"
    image="player.pl.resizer"
    resize="bottomright"
  />

