    tooltip="Double Size"
    rectrgn="1"
  />


  <button
    id="button.vid.tv"
    action="VID_TV"
    x="-183" y="3"
    relatx="1"
    image="player.button.video.tv"
    downImage="player.button.video.tv.pressed"
    hoverImage="player.button.video.tv.hover"
    tooltip="Internet TV"
    rectrgn="1"
  />

  <button
    id="button.vid.misc"
    action="VID_Misc"
    x="-158" y="3"
