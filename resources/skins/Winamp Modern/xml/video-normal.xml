<groupdef id="video.buttons.group" relatw="1" w="0" h="21">
  <layer
    x="0" y="0"
    image="player.button.video.bg.detached.left"
  />
  <layer
    x="74" y="0"
    relatw="1" w="-264"
    image="player.button.video.bg.detached.center"
